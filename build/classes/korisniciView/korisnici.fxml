<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="korisniciController.KorisniciController">
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Region maxHeight="-Infinity" prefHeight="18.0" prefWidth="243.0" HBox.hgrow="ALWAYS" />
            <Label minWidth="-Infinity" text="Korisnici:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Region maxHeight="-Infinity" prefHeight="20.0" prefWidth="286.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </top>
   <center>
      <TableView fx:id="tablica" minWidth="-Infinity" prefHeight="250.0" prefWidth="194.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="ime" prefWidth="250.0" text="Ime" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#odaberi" text="Odaberi" />
            <Region maxHeight="-Infinity" prefHeight="23.0" prefWidth="334.0" HBox.hgrow="ALWAYS" />
            <Button layoutX="20.0" layoutY="20.0" onAction="#kreiraj" mnemonicParsing="false" text="Kreiraj novog korisnika" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
